	<servlet>
		<servlet-name>AllBorrowAppUsers</servlet-name>
		<servlet-class>AllBorrowAppUsersServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AllBorrowAppUsers</servlet-name>
		<url-pattern>/borrowappusers</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AllBorrowAppProducts</servlet-name>
