		<servlet-name>AllBorrowAppProducts</servlet-name>
		<url-pattern>/borrowappproducts</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AllBorrowAppTransactions</servlet-name>
		<servlet-class>AllBorrowAppTransactionsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AllBorrowAppTransactions</servlet-name>
		<url-pattern>/borrowapptransactions</url-pattern>
	</servlet-mapping>
</web-app>
